<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>病历</title>
    <link rel="stylesheet" href="../web/../css/reset.css">
    <link rel="stylesheet" href="../web/../css/hf.css">
    <link rel="stylesheet" href="../web/../css/case.css">
    <script src="../js/jquery-1.11.3.js"></script>
    <script src="../js/case.js"></script>

</head>
<body>
<!--页头-->
<%- include('../include/header') -%>
<div class="main">
    <div class="main-box  container">
        <!--头部-->
        <div class="main_header_box">
            <div id="main_header">
                <p class="title">病历</p>
            </div>
        </div>
        <!--主体-->
        <div class="main_content">
            <div class="p_infor lf">
                <p class="p_title">
                    患者信息
                </p>
                <div class="p_content">
                    <div class="p_img lf"></div>
                    <ul class="p_infor1 lf">
                        <li><b class="realName"></b></li>
                        <li><i>性别：</i><span class="gender"></span></li>
                        <li><i>电话：</i><span class="phoneNo"></span></li>
                    </ul>
                    <ul class="p_infor2 lf">
                        <li><i>年&nbsp;&nbsp;&nbsp;龄：</i><span class="age"></span></li>
                        <li><i>身份证：</i><span class="idCardNo"></span></li>
                        <li><i>职&nbsp;&nbsp;&nbsp;业：</i><span class="occupation"></span></li>
                    </ul>
                </div>
                <ul class="more">
                    <li><i>住址：</i><span class="address"></span></li>
                    <li><i>现病史：</i><span class="presentIllnessHistory"></span></li>
                    <li><i>个人史：</i><span class="personalIllnessHistory"></span></li>
                    <li><i>既往史：</i><span class="previousIllnessHistory"></span></li>
                    <li><i>家族史：</i><span class="familyIllnessHistory"></span></li>
                    <li><i>主&nbsp;&nbsp;诉：</i><span class="chiefComplaint"></span></li>

                </ul>
            </div>
            <div class="disease lf">
                <!--诊断-->
                <div class="diagnosis">
                    <div class="about1">
                        <p class="slide">
                          <span class="title_slide">疾病诊断</span>
                          <!--<a href="javascript:;" class="btn-slide xs1">收起</a>-->
                            <!--<a href="javascript:;" class="btn-slide xs2"  style="display:none;">展开</a>-->
                        </p>
                        <div id="panel1">
                            <ul class="det">
                            </ul>
                        </div>
                    </div>
                    <div class="about2">
                        <p class="slide">
                            <span class="title_slide">中医分型</span>
                            <!--<a href="javascript:;" class="btn-slide xs1" >展开</a>-->
                            <!--<a href="javascript:;" class="btn-slide xs2" style="display:none;">收起</a>-->
                        </p>
                        <div id="panel2">
                            <ul class="det">
                            </ul>
                        </div>
                    </div>
                </div>
                <!--治疗方案-->
                <div class="treatment">
                    <p class="p_title">
                            治疗方案
                    </p>
                    <p class="treatment_det">
                    </p>
                </div>
            </div>
            <div class="rt finish_box"><a href="indexNPage" class="finish">返回首页</a></div>
        </div>
    </div>
</div>
<!--页尾-->
<%- include('../include/footer') -%>
<script>
    $(document).ready(function(){
        $(".about1 .btn-slide").click(function(){
            $("#panel1").slideToggle("slow");
            $(this).toggleClass("active");
            $(".about1 .xs1").toggle();
            $(".about1 .xs2").toggle();
            return false;
        });
    });
    $(document).ready(function(){
        $(".about2 .btn-slide").click(function(){
            $("#panel2").slideToggle("slow");
            $(this).toggleClass("active");
            $(".about2 .xs1").toggle();
            $(".about2 .xs2").toggle();
            return false;
        });
    });
</script>
</body>
<script src="../js/sharing.js"></script>
</html>